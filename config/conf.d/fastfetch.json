{
    "$schema": "../schema.json",
    "app_key": "Fastfetch",
    "enabled": true,
    "application": {
        "name": "Fastfetch",
        "type": "github_deb",
        "package_name": "fastfetch",
        "repo_owner": "fastfetch-cli",
        "repo_name": "fastfetch",
        "filename_pattern_template": "fastfetch-linux-amd64.deb",
        "checksum_algorithm": "sha256",
        "checksum_from_github_release_digest": true,
        "skip_checksum": 0,
        "allow_insecure_http": false
    }
}
